{
  "name": "milo-discord-dashboard",
  "version": "2.0.0",
  "description": "Milo's Discord bot platform with public site, dashboard, admin panel, payments, and 18+ verification",
  "main": "backend/index.js",
  "scripts": {
    "start": "node backend/index.js",
    "start:prod": "NODE_ENV=production node backend/production.js",
    "dev": "nodemon backend/index.js",
    "bot": "node bot/index.js",
    "bot:dev": "nodemon bot/index.js",
    "web": "cd frontend && npm start",
    "web:dev": "cd frontend && npm run dev",
    "web:build": "cd frontend && npm run build",
    "build": "npm run web:build",
    "build:prod": "npm run web:build && npm run optimize",
    "install:all": "npm install && cd frontend && npm install && cd backend && npm install",
    "start:all": "concurrently \"npm run bot\" \"npm run start\" \"npm run web\"",
    "dev:all": "concurrently \"npm run bot:dev\" \"npm run dev\" \"npm run web:dev\"",
    "prod:setup": "npm run install:all && npm run build:prod",
    "prod:start": "npm run start:prod",
    "optimize": "echo 'Frontend optimized for production'",
    "deploy:check": "npm run build && node scripts/deploy-check.js",
    "logs": "tail -f logs/production.log",
    "setup": "node setup-server.js",
    "setup:wizard": "node setup-server.js",
    "deploy:docker": "docker-compose up -d --build",
    "deploy:pm2": "./deploy-pm2.sh",
    "deploy:stop": "docker-compose down || pm2 stop ecosystem.config.js",
    "deploy:restart": "docker-compose restart || pm2 restart ecosystem.config.js",
    "deploy:logs": "docker-compose logs -f || pm2 logs",
    "backend:start": "cd backend && npm start",
    "backend:dev": "cd backend && npm run dev",
    "backend:build": "cd backend && npm install --production",
    "frontend:start": "cd frontend && npm start",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "servers:start": "concurrently \"npm run backend:start\" \"npm run frontend:start\"",
    "servers:dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "health": "node scripts/health-check.js",
    "health:check": "curl -f http://localhost:8000/api/health || echo 'Backend not running'",
    "health:schedule": "node scripts/health-check.js --schedule",
    "ssl:setup": "./setup-ssl.sh",
    "backup": "node scripts/backup.js",
    "backup:create": "node scripts/backup.js",
    "restore": "node scripts/restore.js",
    "backup:restore": "node scripts/restore.js",
    "deploy": "node scripts/deploy.js",
    "security": "node scripts/security-scan.js",
    "security:scan": "node scripts/security-scan.js",
    "database": "node scripts/database-manager.js",
    "db:backup": "node scripts/database-manager.js",
    "db:restore": "node scripts/database-manager.js",
    "db:stats": "node scripts/database-manager.js",
    "test:e2e": "npm run servers:start && npm run test:integration",
    "test:integration": "echo 'Integration tests would run here'",
    "railway:build": "npm run install:all && npm run frontend:build",
    "railway:start": "npm run backend:start",
    "build:all": "npm run install:all && npm run frontend:build",
    "prod": "NODE_ENV=production npm run backend:start"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mongoose": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "ytdl-core": "^4.11.5",
    "@discordjs/voice": "^0.16.1",
    "play-dl": "^1.9.7",
    "canvas": "^2.11.2",
    "discord-player": "^6.6.6",
    "socket.io": "^4.7.4",
    "stripe": "^14.9.0",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "connect-mongo": "^5.1.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "express-validator": "^7.0.1",
    "sharp": "^0.33.0",
    "nodemailer": "^6.9.7",
    "winston": "^3.11.0",
    "moment": "^2.29.4",
    "ejs": "^3.1.9",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "discord",
    "bot",
    "dashboard",
    "moderation",
    "music",
    "web",
    "milo"
  ],
  "author": "Milo",
  "license": "MIT"
}